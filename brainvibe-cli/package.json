{
  "name": "brainvibe-cli",
  "version": "1.0.0",
  "description": "A command-line tool for tracking learning progress and visualizing your knowledge graph",
  "main": "dist/index.js",
  "bin": {
    "brainvibe": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build-hook": "node scripts/build-hook.js",
    "start": "node dist/index.js",
    "dev": "node scripts/dev.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "node scripts/clean.js",
    "deep-clean": "node scripts/clean.js --deep",
    "prepare": "node scripts/prepare.js",
    "install-global": "node scripts/install.js",
    "uninstall-global": "node scripts/uninstall.js",
    "publish": "node scripts/publish.js"
  },
  "keywords": [
    "learning",
    "progress",
    "tracking",
    "git",
    "cli"
  ],
  "author": "BrainVibe",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.6.0",
    "commander": "^11.1.0",
    "inquirer": "^8.2.4",
    "node-fetch": "^2.7.0",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.7",
    "@types/commander": "^2.12.5",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brainvibe/brainvibe-cli.git"
  },
  "bugs": {
    "url": "https://github.com/brainvibe/brainvibe-cli/issues"
  },
  "homepage": "https://github.com/brainvibe/brainvibe-cli#readme"
}
